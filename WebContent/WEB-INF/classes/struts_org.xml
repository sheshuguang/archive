<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- 帐户组管理 -->
	<package name="org" extends="struts-default" >
		
		<!-- 读取帐户组树 -->
		<action name="orgtreeAction" class="orgAction" method="loadOrgTreeData"></action>
		<!-- 新建账户组 -->
		<action name="addOrg" class="orgAction" method="addOrg"></action>
		<!-- 修改账户组 -->
		<action name="editOrg" class="orgAction" method="updateOrg"></action>
		<!-- 删除账户组 -->
		<action name="delOrg" class="orgAction" method="deleteOrg"></action>
		<!-- 移动账户组 -->
		<action name="moveOrg" class="orgAction" method="moveOrg"></action>
	</package>
</struts>
